{
  "name": "@openlab/catalyst-vue-webapp",
  "private": true,
  "version": "1.0.2",
  "description": "A Vue web app for browsing, searching and viewing Not-Equal Catalyst projects",
  "repository": "unplatform/catalyst-vue-webapp",
  "author": "Rob Anderson (https://r0b.io)",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:server -- --silent && npm run build:client -- --no-clean --silent",
    "build:client": "vue-cli-service build",
    "build:server": "WEBPACK_TARGET=node vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write '**/*.{js,json,css,md,vue}'",
    "gen-readme-toc": "npx @robb_j/md-toc -i"
  },
  "keywords": [
    "not-equal-catalyst",
    "vue",
    "webapp"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "casex": "^3.0.0",
    "core-js": "^3.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "marked": "^0.7.0",
    "redis": "^2.8.0",
    "serve-favicon": "^2.5.0",
    "stoppable": "^1.1.0",
    "valid-env": "^1.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.3.1",
    "@types/lodash.isequal": "^4.5.5",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "bulma": "^0.8.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "ky": "^0.16.1",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-svg-loader": "^0.15.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-node-externals": "^1.7.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md,vue}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
