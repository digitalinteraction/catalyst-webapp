{
  "name": "vue-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:server -- --silent && npm run build:client -- --no-clean --silent",
    "build:client": "vue-cli-service build",
    "build:server": "WEBPACK_TARGET=node vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write '**/*.{js,json,css,md,vue}'",
    "gen-readme-toc": "npx @robb_j/md-toc -i"
  },
  "dependencies": {
    "@vue/babel-preset-app": "^3.4.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "marked": "^0.6.1",
    "redis": "^2.8.0",
    "serve-favicon": "^2.5.0",
    "stoppable": "^1.1.0",
    "valid-env": "^0.1.3",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.6.7",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "bulma": "^0.7.4",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^3.1.1",
    "husky": "^1.3.1",
    "ky": "^0.9.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.16.0",
    "sass-loader": "^7.1.0",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.5.21",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,vue}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,vue}": [
      "eslint",
      "git add"
    ]
  }
}
